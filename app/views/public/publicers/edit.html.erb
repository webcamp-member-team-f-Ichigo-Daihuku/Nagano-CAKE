<h2>会員情報編集</h2>
<%= form_with model: @publicer, url: public_publicer_path, method: :patch do |f| %>

  <div class="field">
    <%= f.label :名前 %><br />
    <%= f.label :"(姓)" %><br />
    <%= f.text_field :last_name, autofocus: true, autocomplete: "email" %><br />
    <%= f.label :"(名)" %><br />
    <%= f.text_field :first_name, autofocus: true, autocomplete: "email" %>
  </div>

  <div class="field">
    <%= f.label :フリガナ %><br />
    <%= f.label :"(セイ)" %><br />
    <%= f.text_field :last_name_kana, autofocus: true, autocomplete: "email" %><br />
    <%= f.label :"(メイ)" %><br />
    <%= f.text_field :first_name_kana, autofocus: true, autocomplete: "email" %>
  </div>

  <div class="field">
    <%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
  </div>

  <div class="field">
    <%= f.label :"郵便番号(ハイフンなし)" %><br />
    <%= f.text_field :postal_code, autofocus: true, autocomplete: "email" %>
  </div>

  <div class="field">
    <%= f.label :住所 %><br />
    <%= f.text_field :address, autofocus: true, autocomplete: "email" %>
  </div>

  <div class="field">
    <%= f.label :"電話番号（ハイフンなし）" %><br />
    <%= f.text_field :telephone_number, autofocus: true, autocomplete: "email" %>
  </div>

  <%= f.submit "編集内容を保存" %>
  <%= link_to "退会する", public_publicers_check_path(@publicer) %>

<% end %>